{
	"schema": "a2demo",
	"model": "Invoice",
	"actions": {
		"index": {
			"index": true,
			"template": "index.template",
			"view": "index.view",
			"export": {
				"format": "xlsx",
				"template": "index.template",
				"fileName": "Перелік документів"
			}
		},
		"edit": {
			"model": "Document",
			/*
      "parameters": {
        "Kind": "TEST"
      },*/
			"view": "invoice.view",
			"template": "invoice.template"
		},
		"empty": {
			"model": "Empty",
			"view": "empty.view",
			"template": "empty.template"
		}
	},
	"commands": {
		"createShipment": {
			"type": "sql",
			"procedure": "Invoice.CreateShipment"
		},
		"fetchCustomer": {
			"type": "sql",
			"procedure": "Agent.Fetch"
		},
		"testCommand": {
			"type": "sql",
			"procedure": "Agent.Fetch",
			"parameters": {
				"Kind": "Customer"
			}
		}
	},
	"dialogs": {
		"wizard": {
			"model": "Agent",
			"view": "demo.wizard",
			"template": "/Agent/editCustomer.template"
		},
		"uploadWizard": {
			"model": "Upload",
			"view": "upload.wizard",
			"template": "upload.template"
		},
		"emptyDialog": {
			"model": "Empty",
			"view": "empty.dialog"
		}
	},
	"reports": {
		"invoice": {
			"type": "stimulsoft",
			"report": "invoice",
			"model": "Document",
			"name": "@[Invoice] {{Document.No}}"
		},
		"testxml": {
			"type": "xml",
			"model": "TestXml",
			"name": "{{Params.Name}}",
			"xmlSchemas": [ "F0103306", "../common_types" ],
			"encoding": "windows-1251"
		}
	},
	"uploads": {
		"uploadFile": {
			"model": "Test.Upload",
			"parse": "excel"
		}
	}
}
